  procedures :
   -
     name : ModifModeFenetre
     internal_properties : CAAAAAgAAABGmkTW8OS69PotaOis2BsLEWbrL5FEMXpuT/1/a0vjYXx3OozgBco3RNyIl/zdJ6wHr7TooWWOPB+34mRvKeZhD7PDauaMPXaSSkKUMEBHGOZxzWgz48DVwQQKoTRzEQj6IQRNHAtXIUGydSQ/Kr9iAbO2KdgT+UJjygBfuY7Ub/EAz8I0Ho5jNtGVBhSg27Hztko7
     procedure_id : 1877285253960098496
     type_code : 14
     code : |1-
      // Modification du mode de la fenêtre
      // Entrée:	ModeFenetre=mode d'ouverture de la fenêtre :
      //				- "Parcours"	Visualisation de tous les enregistrements
      //								grâce aux boutons de parcours
      //				- "Modif"		Modification de l'enregistrement en cours
      //				- "Création"	Création d'un nouvel enregistrement
      PROCEDURE ModifModeFenetre(ModeFenetre)
      
      // stocke le mode de la fenêtre
      gsModeFenetre=ModeFenetre
      
      // passage en mode parcours
      SI ModeFenetre=<§$0000§> ALORS
      	// boutons du mode parcours actifs
      	GP_PARCOURS..visible=vrai
      	// boutons du mode modification 
      	GP_MODIF..visible=faux
      	// positionnement sur le premier enregistrement
      	HLitPremier(Clients,Codeclient)
      SINON
      	// boutons du mode parcours 
      	GP_PARCOURS..visible=faux
      	// boutons du mode modification actif
      	GP_MODIF..visible=vrai
      	// si passage en mode création
      	SI ModeFenetre=<§$0003§> ALORS
      		// vide les champs
      		HRAZ(Clients)
      		SAI_CodeClient..Etat=actif
      	FIN
      FIN
      
      // transfert du buffer du fichier vers les champs de la fenêtre
      FichierVersEcran()
     type : 458752
   -
     name : VerifModification
     internal_properties : CAAAAAgAAAA5ecL45scAvNuo62ti4xXf4ZIFRanrPzw+S8zGmYP3STmJJeC0Aeh3GNKECc7RUtWYPHb3Dowxbhw9RTHvNF+sB/Zgknqcyo2WQ8owWXpCMY/g4PUzpy2N1VO8DsNJAP9wzceGPGOyZhXnrexaYdUnJcmHM15ZiM9Xtbueh3YP1tEwOA==
     procedure_id : 1877285253960164032
     type_code : 14
     code : |1+
      // Vérification des modifications de la fiche
      // Si la fiche a été modifiée, demande confirmation
      // des modifications à l'utilisateur
      // Modifie l'enregistrement si confirmation// Traitement automatique des erreurs :   afficher l'erreur puis reprendre la saisie
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE VerifModification()
      
      // si un champ a été modifiée dans la fiche
      SI MaFenetre..Modifié ALORS
      	// indique que la fiche n'est plus modifiée
      	MaFenetre..Modifié=faux
      
      	SI OuiNon(<§$0004§>) ALORS
      		// transfert des champs dans le buffer fichier
      		EcranVersFichier()
      		// modification de l'enregistrement
      		HModifie(Clients)
      
      	FIN
      FIN
      
      CAS EXCEPTION:
      	AfficheErreur(ExceptionInfo(errMessage))
      	RepriseSaisie()
     type : 458752
